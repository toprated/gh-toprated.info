@{
    Layout = null;
    ViewBag.Title = "MainLogo";
    const string ff = "Arial";
    const string textColor = "white";//"#f8f8f8";
}

<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="80px" height="80px" viewBox="0 0 80 80">
    <rect id="logo-background" x="0" y="0" width="80" height="80" style="fill: none; stroke: none;" />
    <circle id="logo-circle" cx="40" cy="40" stroke="black" style="stroke-width: 0;" r="40" fill="#FFA500" />
    <text fill="@textColor" x="4" y="57" id="logo-t-text" style="font-family: @ff; font-size: 63px; font-style: normal;" dy="" dx="">T</text>
    <text fill="@textColor" x="32" y="72" id="logo-r-text" style="font-family: @ff; font-size: 63px; font-style: normal;" dy="" dx="">R</text>
</svg>
